{
    "name": "phpbenchmarks/benchmark-kit",
    "description": "PHPBenchmarks kit to add benchmarks",
    "license": "proprietary",
    "authors": [
        {
            "name": "Steevan BARBOYON",
            "email": "steevan.barboyon@gmail.com",
            "homepage": "http://www.info-droid.fr",
            "role": "Project lead"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-zend-opcache": "*",
        "huttopia/console-bundle": "1.1.*",
        "steevanb/php-typed-array": "2.1.*",
        "steevanb/symfony-options-resolver": "^1.1",
        "symfony/config": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dependency-injection": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/filesystem": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/process": "4.4.*",
        "symfony/routing": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/yaml": "4.4.*",
        "twig/twig": "3.0.*"
    },
    "require-dev": {
        "phpstan/phpstan": "0.11.*",
        "steevanb/php-backtrace": "2.0.*",
        "symfony/var-dumper": "4.4.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
