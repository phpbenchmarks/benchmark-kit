{
    "name": "phpbenchmarks/benchmark-kit",
    "description": "PHPBenchmarks kit to add benchmarks",
    "license": "proprietary",
    "authors": [
        {
            "name": "Steevan BARBOYON",
            "email": "steevan.barboyon@gmail.com",
            "homepage": "http://www.info-droid.fr",
            "role": "Project lead"
        }
    ],
    "require": {
        "php": "7.3.*",
        "ext-json": "*",
        "ext-curl": "*",
        "symfony/console": "4.3.*",
        "symfony/validator": "4.3.*"
    },
    "require-dev": {
        "symfony/var-dumper": "4.3.*",
        "phpstan/phpstan": "0.11.*",
        "maglnet/composer-require-checker": "2.0.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "AbstractComponentConfiguration\\": "/var/www/phpbenchmarks/.phpbenchmarks"
        }
    }
}
